{
   "name": "cms",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "node build/index.js",
      "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' -r dotenv/config --exec 'ts-node --files' src/index.ts",
      "build": "rm -rf build && tsc",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:create": "cd src/db/migration &&  typeorm-ts-node-commonjs migration:create",
      "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/db/dbConnection",
      "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/db/dbConnection",
      "lint": "eslint 'src/**/*.ts'",
      "lint:fix": "eslint 'src/**/*.ts' --fix"
   },
   "author": "",
   "license": "ISC",
   "dependencies": {
      "@aws-sdk/client-secrets-manager": "^3.441.0",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "helmet": "^7.0.0",
      "joi": "^17.11.0",
      "jsonwebtoken": "^9.0.2",
      "morgan": "^1.10.0",
      "pg": "^8.11.3",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.0",
      "tsconfig-paths": "^4.2.0",
      "typeorm": "0.3.17",
      "winston": "^3.11.0"
   },
   "devDependencies": {
      "@types/cors": "^2.8.15",
      "@types/express": "^4.17.20",
      "@types/morgan": "^1.9.7",
