      "@typescript-eslint/eslint-plugin": "^6.9.1",
      "@typescript-eslint/parser": "^6.9.1",
      "eslint": "^8.52.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "eslint-plugin-simple-import-sort": "^10.0.0",
      "eslint-plugin-unused-imports": "^3.0.0",
      "nodemon": "^3.0.1",
      "ts-node": "10.9.1",
      "typescript": "5.2.2"
   }
}
